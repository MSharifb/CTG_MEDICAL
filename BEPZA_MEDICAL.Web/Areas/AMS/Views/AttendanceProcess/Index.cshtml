@using ERP_BEPZA.Web.Areas.AMS.Models.AttendanceProcess
@model AttendanceProcessViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/AMS/Views/Shared/_Layout.cshtml";
}   
@using (Html.BeginForm("Create", "AttendanceProcess", FormMethod.Post, new { id = "frm" }))
{
<fieldset>
    <div class="GroupBox" id="message">
        @Html.ValidationSummary(false, ERP_BEPZA.Web.Utility.Common.ValidationSummaryHead)
        @if (!string.IsNullOrEmpty(Model.ErrMsg))
        {
            <div style="color:Red" id="ErrMsg" class="@Model.ErrMsg">
                @Model.ErrMsg
            </div>
        }

    </div>

    @Html.Partial("_CreateOrEdit", Model)

    <div class="button-crude">
        @if ((new ERP_BEPZA.Web.Utility.AppConstant()).IsAddAssigned)
        {
            <input type="submit" value="Process" id="btnSave" name="btnSubmit" style="margin-right: 8px;" />
        }
     
    </div>
    
</fieldset>

}