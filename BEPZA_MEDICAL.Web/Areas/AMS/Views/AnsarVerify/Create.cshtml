@model BEPZA_MEDICAL.Web.Areas.AMS.ViewModel.AnsarVerifyViewModel
@{
    ViewBag.Title = "Create";
    Layout = "~/Areas/AMS/Views/Shared/_LayoutNew.cshtml";
}

@using (Html.BeginForm("Create", "AnsarVerify", FormMethod.Post, new { id = "frm" }))
{
    @Html.Partial("_CreateOrEdit", Model)
    <div class="form-horizontal">
        <div class="form-group">
            <div class="col-sm-9 col-sm-offset-3 text-right">
                @if ((new BEPZA_MEDICAL.Web.Utility.AppConstant()).IsAddAssigned)
                {
                    <button class="btn btn-success" id="btnSave"><i class="fa fa-save"></i> Verify</button>
                    <button class="btn btn-success" id="btnCancel"><i class="fa fa-save"></i> Reject</button>
                }
                <a href="@Url.Action("Index")" class="btn btn-warning"> <i class="fa fa-backward"></i> Back</a>
            </div>
        </div>
    </div>

}



