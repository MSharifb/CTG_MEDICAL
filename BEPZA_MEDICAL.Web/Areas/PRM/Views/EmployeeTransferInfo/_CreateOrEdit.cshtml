                    $('#EmployeeName').val(obj.EmployeeName);
                    $('#JoiningDate').val(obj.JoiningDate);

                    $('#FromDisciplineId').val(obj.FromDisciplineId);
                    $('#ToDisciplineId').val(obj.FromDisciplineId);

                    $('#FromDivisionName').val(obj.FromDivisionName);
