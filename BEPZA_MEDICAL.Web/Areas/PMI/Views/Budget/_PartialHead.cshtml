@model List<BEPZA_MEDICAL.Web.Areas.PMI.ViewModel.Budget.YearWiseBudgetStatusViewModel>
@using BEPZA_MEDICAL.Web.Helpers

@foreach (var item in Model.AsEnumerable())
{
    @Html.Raw("<th class='yearly-cost-head'>")
    using (Html.BeginCollectionItem("YearWiseBudgetStatusList"))
    {
        @Html.Partial("_PartialHeadItem", @item)
    }
    @Html.Raw("</th>")
}
