@model BEPZA_MEDICAL.Web.Areas.CPF.Models.ProfitInterestRate.ProfitInterestRateModel

@{
    ViewBag.Title = "Create";
    Layout = "~/Areas/CPF/Views/Shared/_Layout.cshtml";
}

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm("Edit", "ProfitInterestRate", FormMethod.Post, new { id = "frm" }))
{
    <div class="GroupBox" style="max-height: 15px" id="message">
        @if (Model != null && !string.IsNullOrEmpty(Model.ErrMsg))
        {
            <div class="@Model.errClass">
                @Model.ErrMsg
            </div>
        }
    </div>

    <fieldset>
        @Html.HiddenFor(model => model.Id)
        @Html.Partial("_CreateOrEdit", Model)

        <br />
        <div class="button-center buttonSubmit">
            @if ((new BEPZA_MEDICAL.Web.Utility.AppConstant()).IsEditAssigned)
            {
                <input type="submit" value="Update" style="width: auto; overflow: visible; padding-left: .25em; padding-right: .25em;" />
            }
        </div>
    </fieldset>
}

<div class="button-crude button-left" style="padding-left: 5px;">
    <span></span>
    @Html.ActionLink("Back to List", "Index")
</div>

