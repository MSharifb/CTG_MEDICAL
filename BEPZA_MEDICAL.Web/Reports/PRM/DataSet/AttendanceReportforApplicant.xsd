          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="EREC_uspReportUniversitywiseApplicantsTableAdapter" GeneratorDataComponentClassName="EREC_uspReportUniversitywiseApplicantsTableAdapter" Name="EREC_uspReportUniversitywiseApplicants" UserDataComponentName="EREC_uspReportUniversitywiseApplicantsTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="DBConnectionString (Web.config)" DbObjectName="SELECT g.*,a.PresentAddress,d.name HomeDistrict,a.PermanentDistrict, a.PermanentThana, ad.AdCode,c.JobAdvertisementInfoId, CAST(SUBSTRING(g.age, 0,3) AS INT) agenum, u.Name universityname,'' universityid FROM EREC_tblgeneralinfo g JOIN EREC_tbleducationalinfo e ON g.intPK=e.GeneralID JOIN EREC_tblmaritalinfo m ON g.intPK=m.GeneralID JOIN EREC_tbladdress a ON g.intPK=a.GeneralID JOIN EREC_tblDistrict d ON a.PermanentDistrict=d.intPK JOIN PRM_JobAdvertisementPostDetail c ON g.CircularID=c.Id JOIN PRM_JobAdvertisementInfo ad ON c.JobAdvertisementInfoId = ad.Id JOIN PRM_UniversityAndBoard u ON 1 = u.Id" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select AdCode, rollno,name, DateOfBirth, NationalId, MobilePhoneNo, FathersName, FathersOccupation, 
		MothersName, MothersOccupation,PresentAddress,  HomeDistrict,  gender,agenum, universityname
		from
		(select g.*,a.PresentAddress,d.name HomeDistrict,a.PermanentDistrict, a.PermanentThana,
		ad.AdCode,c.JobAdvertisementInfoId, CAST(SUBSTRING(g.age, 0,3) AS INT)  agenum, u.Name universityname,'' universityid
		from EREC_tblgeneralinfo g 
		join EREC_tbleducationalinfo e on g.intPK=e.GeneralID
		join EREC_tblmaritalinfo m on g.intPK=m.GeneralID 
		join EREC_tbladdress a on g.intPK=a.GeneralID 
		join EREC_tblDistrict d on a.PermanentDistrict=d.intPK
		join PRM_JobAdvertisementPostDetail c on g.CircularID=c.Id 
		JOIN PRM_JobAdvertisementInfo ad on c.JobAdvertisementInfoId = ad.Id
		join PRM_UniversityAndBoard u on 1 = u.Id
		) f 
		where 
		f.status=1</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
